<?xml version="1.0" encoding="UTF-8"?>
<web-app>
    <listener>
        <listener-class>us.kbase.narrativejobservice.AppEventListener</listener-class>
    </listener>

    <listener>
        <listener-class>us.kbase.narrativejobservice.ReaperServiceServlet</listener-class>
    </listener>

    <servlet>
        <servlet-name>RootServlet</servlet-name>
        <servlet-class>us.kbase.narrativejobservice.NarrativeJobServiceServer</servlet-class>
        <load-on-startup>1</load-on-startup>

    </servlet>
    <servlet-mapping>
        <servlet-name>RootServlet</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>
</web-app>

